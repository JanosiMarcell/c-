using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace jelszovalami
{
    class Program
    {
        static bool Szamjegy(string jsz)
        {
            bool s = false;
            string chs = "0123456789";
            int i = 0;
            while ((!s)&& (i < chs.Length))
            {
                if (jsz.IndexOf(chs[i])>=0)
                {
                    s = true;
                }
                i++;
            }
            return (s);
        }
        static bool Kisbetu(string jsz)
        {
            bool s = false;
            string chs = "aábcdeéfghiíjklmnopoóöőüűuúpqrstvwxyz";
            int i = 0;
            while ((!s) && (i < chs.Length))
            {
                if (jsz.IndexOf(chs[i]) >= 0)
                {
                     s = true;
                }
                i++;
            }
            return (s);
        }
        static bool Nagyetu(string jsz)
        {
            bool s = false;
            string chs = "AÁBCDEÉFGHIÍJKLMNOÓÖŐÜŰPQRSTUÚVWXYZ";
            int i = 0;
            while ((!s) && (i < chs.Length))
            {
                if (jsz.IndexOf(chs[i]) >= 0)
                {
                    s = true;
                }
                i++;
            }
            return (s);
        }
        static void Main(string[] args)
        {
            Console.WriteLine("A jelszó: ");
            string jelszo = Console.ReadLine();
            
        }

    }
}
